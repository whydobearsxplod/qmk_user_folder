// LETTERS //

COMB(B,KC_B,ARTSEY_E,ARTSEY_Y,ARTSEY_O)
COMB(BT,KC_B,ARTSEY_E,ARTSEY_O)
COMB(C,KC_C,ARTSEY_E,ARTSEY_Y)
COMB(D,KC_D,ARTSEY_A,ARTSEY_R,ARTSEY_S)
COMB(DT,KC_D,ARTSEY_A,ARTSEY_R,ARTSEY_T)
COMB(F,KC_F,ARTSEY_A,ARTSEY_R)
COMB(G,KC_G,ARTSEY_R,ARTSEY_T)
COMB(H,KC_H,ARTSEY_A,ARTSEY_T)
COMB(HT,KC_H,ARTSEY_E,ARTSEY_I)
COMB(U,KC_U,ARTSEY_Y,ARTSEY_I)
COMB(K,KC_K,ARTSEY_Y,ARTSEY_O)
COMB(L,KC_L,ARTSEY_E,ARTSEY_Y,ARTSEY_I)
COMB(M,KC_M,ARTSEY_Y,ARTSEY_I,ARTSEY_O)
COMB(N,KC_N,ARTSEY_I,ARTSEY_O)
COMB(P,KC_P,ARTSEY_E,ARTSEY_I,ARTSEY_O)
COMB(Q,KC_Q,ARTSEY_A,ARTSEY_T,ARTSEY_S)
COMB(J,KC_J,ARTSEY_T,ARTSEY_S)
COMB(V,KC_V,ARTSEY_R,ARTSEY_S)
COMB(W,KC_W,ARTSEY_A,ARTSEY_S)
COMB(X,KC_X,ARTSEY_R,ARTSEY_T,ARTSEY_S)
COMB(Z,KC_Z,ARTSEY_A,ARTSEY_R,ARTSEY_T,ARTSEY_S)

// SPACE, TAB, ENTER, BACKSPACE //
COMB(TAB,KC_TAB,ARTSEY_O, ARTSEY_A, ARTSEY_R, ARTSEY_T)
COMB(ESC,KC_ESC,ARTSEY_O, ARTSEY_A, ARTSEY_R)
COMB(sart,KC_SPC,ARTSEY_E,ARTSEY_Y,ARTSEY_I,ARTSEY_O)
COMB(eart,KC_ENTER,ARTSEY_A,ARTSEY_E)
COMB(bart,KC_BSPC,ARTSEY_R,ARTSEY_E)

// One Shot Mods //
COMB(CTRL,OSM(MOD_LCTL),ARTSEY_S, ARTSEY_E)
COMB(ALT,OSM(MOD_LALT),ARTSEY_S, ARTSEY_I)
COMB(GUI,OSM(MOD_LGUI),ARTSEY_S, ARTSEY_Y)

// SHIFT //
COMB(shiftlock,LOCK_SHIFT,ARTSEY_A, ARTSEY_Y, ARTSEY_I, ARTSEY_O)
COMB(ONESHOTSHIFT,OSM(MOD_LSFT),ARTSEY_E, ARTSEY_R, ARTSEY_T, ARTSEY_S)

// PUNCTUATION COMBOS //
COMB(quote,KC_QUOTE,ARTSEY_Y,ARTSEY_T,ARTSEY_S)
COMB(dot,KC_COMMA,ARTSEY_A,ARTSEY_Y)
COMB(comma,KC_DOT,ARTSEY_A,ARTSEY_I)
COMB(slsh,KC_SLSH,ARTSEY_A,ARTSEY_O)

// NUMBERS //
COMB(seven,KC_7,ARTSEY_1,ARTSEY_2)
COMB(eight,KC_8,ARTSEY_2,ARTSEY_3)
COMB(nine,KC_9,ARTSEY_4,ARTSEY_5)
COMB(zero,KC_0,ARTSEY_5,ARTSEY_6)

COMB(RESETCOMBO,RESET,ARTSEY_E,ARTSEY_S,ARTSEY_A,ARTSEY_O, ARTSEY_R, ARTSEY_T, ARTSEY_Y,ARTSEY_I)


// LAYER LOCKING //
COMB(lockmou,TO(_ARTSEY_MOU),ARTSEY_A, ARTSEY_T, ARTSEY_Y)
COMB(lockmou2,TO(_ARTSEY_BASE),KC_MS_BTN1, KC_MS_DOWN, KC_MS_BTN2)
